{
  "name": "vuttr-frontend",
  "version": "1.0.0",
  "description": "frontend para VUTTR",
  "main": "index.js",
  "author": "jeovazero",
  "license": "MIT",
  "scripts": {
    "dev": "webpack-dev-server --hot --progress --open --config webpack.dev.js",
    "prod": "NODE_ENV='production' webpack --progress --config webpack.prod.js",
    "flow": "flow",
    "lint:fix": "standard --fix --parser babel-eslint --plugin flowtype 'src/**/*.{jsx,js}'",
    "prettier": "prettier-standard 'src/**/*.{jsx,js}'",
    "storybook": "start-storybook -p 9002 -c .storybook",
    "build-storybook": "build-storybook -c .storybook"
  },
  "lint-staged": {
    "*.{jsx,js}": [
      "prettier-standard",
      "standard --fix --parser babel-eslint --plugin flowtype",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "flow && lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.3.3",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@commitlint/cli": "^7.5.2",
    "@commitlint/config-conventional": "^7.5.0",
    "@storybook/addon-actions": "5.0.0-rc.6",
    "@storybook/addon-info": "5.0.0-rc.6",
    "@storybook/addon-knobs": "5.0.0-rc.6",
    "@storybook/addon-options": "5.0.0-rc.6",
    "@storybook/react": "5.0.0-rc.6",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.0.5",
    "babel-plugin-emotion": "^10.0.7",
    "eslint": "^5.14.1",
    "eslint-plugin-flowtype": "^3.4.2",
    "flow-bin": "^0.93.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.4",
    "prettier": "^1.16.4",
    "prettier-standard": "^9.1.1",
    "standard": "^12.0.1",
    "uglifyjs-webpack-plugin": "^2.1.2",
    "webpack": "^4.29.5",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.1.14",
    "webpack-merge": "^4.2.1"
  },
  "dependencies": {
    "@emotion/core": "^10.0.7",
    "@emotion/styled": "^10.0.7",
    "emotion-theming": "^10.0.7",
    "node-fetch": "^2.3.0",
    "react": "^16.8.2",
    "react-dom": "^16.8.2",
    "throttle-debounce": "^2.1.0"
  }
}
